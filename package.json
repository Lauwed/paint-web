{
  "name": "dev-girl-paint",
  "version": "0.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "watch": "tsc && npx prettier . --write && vite build --watch",
    "build": "tsc && npx prettier . --check && vite build",
    "start": "node index.js",
    "prepare": "husky",
    "test": "timeout 5 node --env-file=.env index.js || [ $? -eq 124 ]"
  },
  "description": "Le Web Paint de la Dev Girl Society",
  "dependencies": {
    "canvas": "^3.2.1",
    "express": "^4.18.1",
    "sanitize-html": "^2.17.0",
    "socket.io": "^4.5.2",
    "socket.io-client": "^4.8.3"
  },
  "devDependencies": {
    "@eslint/css": "^0.14.1",
    "@eslint/js": "^9.39.2",
    "@eslint/json": "^0.14.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^17.0.0",
    "husky": "^9.1.7",
    "prettier": "3.7.4",
    "sass": "^1.97.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.53.0",
    "vite": "^7.2.4",
    "vite-plugin-eslint": "^1.8.1"
  }
}
